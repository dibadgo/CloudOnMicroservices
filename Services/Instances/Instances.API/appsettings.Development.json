{
  "RabbitMq": {
    "HostName": "centos7",
    "Port": 5672,
    "Routekey": "incloud.queue.*",
    "Queue": "incloud.queue.log",
    "Exchange": "incloud.exchange",
    "EmiterName": "Instances"
  },
  "urls": {
    "identity": "https://identity-api:6101/"
  },
  "ConnectionStrings": {
    "InstancesDbContext": "Server=(LocalDb)\\MSSQLLocalDB;Database=asp-net.Instances;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Grpc": "Information",
      "Microsoft": "Information"
    }
  }
}
